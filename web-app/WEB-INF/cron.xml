<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/cron/updateTweets</url>
    <description>Update tweets from friends every minute</description>
    <schedule>every 1 minutes</schedule>
  </cron>
  <cron>
    <url>/cron/updateFriends</url>
    <description>Update friends list every hour</description>
    <schedule>every 1 hours</schedule>
  </cron>
  <cron>
    <url>/cron/scanRandomUserTimelines</url>
    <description>Scan random user timelines every 30 minutes</description>
    <schedule>every 30 minutes</schedule>
  </cron>
  <cron>
    <url>/cron/topTweets24</url>
    <description>Mail top tweets of the last 24h 17:45 PDT</description>
    <schedule>every day 17:45</schedule>
  </cron>
  <cron>
    <url>/cron/updateFeeds</url>
    <description>Update the latest and important feed data every 15 minutes</description>
    <schedule>every 15 minutes</schedule>
  </cron>
  <cron>
    <url>/cron/updateMentions</url>
    <description>scan for suggst @username every 2 hours</description>
    <schedule>every 2 hours</schedule>
  </cron>
</cronentries>
